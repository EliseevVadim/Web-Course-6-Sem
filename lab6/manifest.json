{
  "name": "Files originality checker",
  "description": "Плагин для проверки оригинальности текстов",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Проверка уникальности текстов"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}